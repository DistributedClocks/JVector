<?xml version="1.0" encoding="UTF-8"?>
<project name="module_jvector" default="compile.module.jvector">
  <dirname property="module.jvector.basedir" file="${ant.file.module_jvector}"/>
  
  <property name="module.jdk.home.jvector" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.jvector" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.jvector" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.jvector" value="-encoding UTF-8 -source 9 -target 9 ${compiler.args}"/>
  
  <property name="jvector.output.dir" value="undefined"/>
  <property name="jvector.testoutput.dir" value="undefined"/>
  
  <path id="jvector.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="jvector.module.production.classpath">
    <path refid="${module.jdk.classpath.jvector}"/>
  </path>
  
  <path id="jvector.runtime.production.module.classpath"/>
  
  <path id="jvector.module.classpath">
    <path refid="${module.jdk.classpath.jvector}"/>
  </path>
  
  <path id="jvector.runtime.module.classpath"/>
  
  
  <patternset id="excluded.from.module.jvector">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.jvector">
    <patternset refid="excluded.from.module.jvector"/>
  </patternset>
  
  
  <target name="compile.module.jvector" depends="compile.module.jvector.production,compile.module.jvector.tests" description="Compile module JVector"/>
  
  <target name="compile.module.jvector.production" depends="register.custom.compilers" description="Compile module JVector; production classes"/>
  
  <target name="compile.module.jvector.tests" depends="register.custom.compilers,compile.module.jvector.production" description="compile module JVector; test classes" unless="skip.tests"/>
  
  <target name="clean.module.jvector" description="cleanup module">
    <delete dir="${jvector.output.dir}"/>
    <delete dir="${jvector.testoutput.dir}"/>
  </target>
</project>